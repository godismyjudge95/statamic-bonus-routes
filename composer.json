{
    "name": "jacksleight/statamic-bonus-routes",
    "autoload": {
        "psr-4": {
            "JackSleight\\StatamicBonusRoutes\\": "src"
        }
    },
    "extra": {
        "statamic": {
            "name": "Bonus Routes",
            "description": "This Statamic addon allows you to easily define additional dynamic collection and taxonomy routes that can be mounted anywhere."
        },
        "laravel": {
            "providers": [
                "JackSleight\\StatamicBonusRoutes\\ServiceProvider"
            ]
        }
    },
    "require": {
        "statamic/cms": "^3.3"
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    }
}
